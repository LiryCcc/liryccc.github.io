{
  "name": "g",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite dev",
    "format": "prettier . --write --cache",
    "lint": "pnpm lint:code && pnpm lint:format && pnpm lint:spell && pnpm lint:style && pnpm lint:type",
    "lint:code": "eslint . --cache",
    "lint:fix": "pnpm format && pnpm lint:style --fix && pnpm lint:code --fix",
    "lint:format": "prettier . --check --cache",
    "lint:spell": "cspell .",
    "lint:style": "stylelint '**/*.css'",
    "lint:type": "tsc -b",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint",
    "commit-msg": "pnpm commitlint --edit"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.10",
    "@reduxjs/toolkit": "^2.11.2",
    "ahooks": "^3.9.6",
    "dayjs": "^1.11.19",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "normalize.css": "^8.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-redux": "^9.2.0",
    "react-router": "^7.13.0",
    "redux": "^5.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.2",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "4.0.16",
    "cspell": "^9.6.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
